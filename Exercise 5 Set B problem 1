#include <stdio.h>
#include <conio.h>

void main()
{
    int n, i, t1 = 1, t2 = 1, nextTerm;
    printf("Enter number of terms: ");
    scanf("%d", &n);

    printf("Fibonacci Series: ");

    for (i = 1; i <= n; ++i)
    {
        if(i == 1)
        {
            printf("%d ", t1);
            continue;
        }
        if(i == 2)
        {
            printf("%d ", t2);
            continue;
        }
        nextTerm = t1 + t2;
        t1 = t2;
        t2 = nextTerm;
        printf("%d ", nextTerm);
    }
}

