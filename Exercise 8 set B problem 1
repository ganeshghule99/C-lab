#include <stdio.h>

int isPrime(int n) {
    if (n <= 1) return 0;
    for (int i = 2; i <= n/2; i++) {
        if (n % i == 0) return 0;
    }
    return 1;
}

int main() {
    int count = 0, num = 2;
    printf("The first 10 prime numbers are:\n");
    while(count < 10) {
        if(isPrime(num)) {
            printf("%d ", num);
            count++;
        }
        num++;
    }
    return 0;
}

