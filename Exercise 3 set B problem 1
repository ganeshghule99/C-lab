#include <stdio.h>
#include <conio.h>

void main()
{
    char ch;
    printf("Enter a character: ");
    scanf("%c", &ch);

    if(ch >= '0' && ch <= '9')
        printf("The character is a digit");
    else if(ch >= 'a' && ch <= 'z')
        printf("The character is lowercase alphabet");
    else if(ch >= 'A' && ch <= 'Z')
        printf("The character is uppercase alphabet");
    else
        printf("It is a special character");
}

