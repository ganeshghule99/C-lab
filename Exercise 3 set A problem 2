#include <stdio.h>
#include <conio.h>

void main()
{
    int n1, n2, n3;
    printf("Enter three numbers: ");
    scanf("%d%d%d", &n1, &n2, &n3);
    
    if((n1 > n2 && n1 < n3) || (n1 > n3 && n1 < n2))
        printf("%d is between %d and %d", n1, n2, n3);
    else
        printf("The number is not between the other two");
}
