#include <stdio.h>

int main() {
    int n, i, j, count = 1;
    printf("Enter number of lines (n): "); // e.g., 4
    scanf("%d", &n);

    for(i = 1; i <= n; i++) {
        for(j = 1; j <= i; j++) {
            printf("%d ", count);
            count++; // Increment the counter
        }
        printf("\n");
    }
    return 0;
}
