#include <stdio.h>
#include <conio.h>

void main()
{
    int n, sum = 0;
    
    do
    {
        printf("Give number: ");
        scanf("%d", &n);
        
        // Add the entered number to the sum
        sum = sum + n;
        
    } while (n > 0); // Loop repeats if n is positive
    
    printf("Sum is %d", sum);
}
