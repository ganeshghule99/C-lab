#include <stdio.h>

int main() {
    int start, end, i, j, isPrime;
    
    printf("Enter start and end range: ");
    scanf("%d %d", &start, &end);

    printf("Prime numbers: ");
    for(i = start; i <= end; i++) {
        if (i < 2) continue; // 0 and 1 are not prime
        
        isPrime = 1; // Assume it is prime
        for(j = 2; j <= i/2; j++) {
            if(i % j == 0) {
                isPrime = 0; // It is not prime
                break;
            }
        }
        if(isPrime) {
            printf("%d ", i);
        }
    }
    return 0;
}

