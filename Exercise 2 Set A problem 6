#include <stdio.h>

int main() {
    float l, b, h, area, volume;

    printf("Enter length, breadth, and height: ");
    scanf("%f %f %f", &l, &b, &h);

    area = 2 * ((l * b) + (l * h) + (b * h));
    volume = l * b * h;

    printf("Surface Area: %.2f\n", area);
    printf("Volume: %.2f\n", volume);

    return 0;
}
