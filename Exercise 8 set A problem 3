#include <stdio.h>

int isEven(int num) {
    return (num % 2 == 0) ? 1 : 0;
}

int main() {
    int n, val;
    printf("How many numbers? ");
    scanf("%d", &n);
    for(int i=0; i<n; i++) {
        printf("Enter number: ");
        scanf("%d", &val);
        if(isEven(val)) printf("Even\n");
        else printf("Odd\n");
    }
    return 0;
}

