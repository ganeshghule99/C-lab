#include <stdio.h>
#include <conio.h>

void main()
{
    int h, m, s;
    printf("Enter time as Hour Minute Second: ");
    scanf("%d%d%d", &h, &m, &s);

    if(h >= 0 && h < 24 && m >= 0 && m < 60 && s >= 0 && s < 60)
        printf("The time is valid");
    else
        printf("The time is invalid");
}
