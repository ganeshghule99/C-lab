#include <stdio.h>

int main() {
    int amount, n10, n5, n1;

    printf("Enter the amount to be withdrawn: ");
    scanf("%d", &amount);

    n10 = amount / 10;
    amount = amount % 10;

    n5 = amount / 5;
    n1 = amount % 5;

    printf("\nCashier will give:\n");
    printf("10 Rupee Notes: %d\n", n10);
    printf(" 5 Rupee Notes: %d\n", n5);
    printf(" 1 Rupee Notes: %d\n", n1);

    return 0;
}
