#include <stdio.h>
#include <conio.h>

void main()
{
    int n, sum = 0;

    do
    {
        printf("Enter a number: ");
        scanf("%d", &n);
        
        if(n > 0)
        {
            sum = sum + n;
        }
        
    } while(n > 0);
    
    printf("Sum of positive numbers is: %d", sum);
}
