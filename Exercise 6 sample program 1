#include <stdio.h>

void main() {
    int n, line_number, number;

    printf("How many lines: ");
    scanf("%d", &n);

    // Outer loop for the number of lines
    for(line_number = 1; line_number <= n; line_number++) {
        // Inner loop for printing numbers in each line
        for(number = 1; number <= line_number; number++) {
            printf("%d\t", number);
        }
        printf("\n"); // Move to next line after printing numbers
    }
}
