#include <stdio.h>

int main() {
    int num, sum, r, temp;

    printf("Armstrong numbers between 1 and 500 are:\n");

    for(num = 1; num <= 500; num++){
        temp = num;
        sum = 0;

        while(temp != 0){
            r = temp % 10;
            sum = sum + (r * r * r);
            temp = temp / 10;
        }

        if(num == sum)
            printf("%d ", num);
    }

    return 0;
}
